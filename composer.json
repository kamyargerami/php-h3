{
    "name": "michaellindahl/php-h3",
    "description": "H3: The hexagonal hierarchical geospatial indexing system system rewritten in PHP.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Lindahl",
            "email": "me@michaellindahl.com"
        }
    ],
    "require": {
        "php": "^7.4 | ^8.0"
    },
    "scripts": {
        "build": "php build.php"
    },
    "autoload": {
